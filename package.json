{
  "dependencies": {
    "@nuxt/a11y": "1.0.0-alpha.1",
    "@nuxt/content": "3.11.2",
    "@nuxt/eslint": "1.14.0",
    "@nuxt/fonts": "0.13.0",
    "@nuxt/hints": "1.0.0-alpha.6",
    "@nuxt/icon": "2.2.1",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "0.13.2",
    "@tailwindcss/vite": "^4.1.18",
    "@unhead/vue": "^2.1.4",
    "better-sqlite3": "^12.6.2",
    "daisyui": "^5.5.18",
    "drizzle-orm": "^0.45.1",
    "eslint": "^10.0.0",
    "nuxt": "^4.3.1",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@catppuccin/daisyui": "^2.1.1",
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/logos": "^1.2.10",
    "@trunkio/launcher": "^1.3.4",
    "@types/node": "^25.2.2",
    "drizzle-kit": "^0.31.8",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.13.1",
    "node-gyp": "^12.2.0",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  },
  "name": "affirm",
  "packageManager": "bun@1.3.9",
  "private": true,
  "scripts": {
    "build": "bun run nuxt build",
    "deploy": "bun run-s build deploy:wrangler",
    "deploy:nonprod": "bun run-s build deploy:nonprod:wrangler",
    "deploy:nonprod:wrangler": "bun run wrangler versions upload",
    "deploy:wrangler": "bun run wrangler deploy",
    "dev": "bun run nuxt dev",
    "format": "bun run run-s format:prettier format:trunk",
    "format:prettier": "bun run prettier --write .",
    "format:trunk": "bun run trunk fmt -a",
    "generate": "bun run nuxt generate",
    "lint": "bun run run-s lint:eslint lint:trunk lint:types",
    "lint:eslint": "bun run eslint .",
    "lint:eslint:fix": "bun run eslint . --fix",
    "lint:fix": "bun run run-s lint:eslint:fix lint:trunk:fix",
    "lint:trunk": "bun run trunk check -a",
    "lint:trunk:fix": "bun run trunk check -a --fix",
    "lint:types": "bun run tsc --noEmit",
    "postinstall": "bun run-s postinstall:trunk:install postinstall:trunk:upgrade postinstall:prepare types format",
    "postinstall:prepare": "bun run nuxt prepare",
    "postinstall:trunk:install": "bun run trunk install --ci",
    "postinstall:trunk:upgrade": "bun run trunk upgrade --ci",
    "preview": "bun run-s build preview:wrangler",
    "preview:wrangler": "bun run wrangler dev",
    "types": "bun run wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:migrate:staging": "wrangler d1 migrations apply DB --env staging --remote",
    "db:migrate:prod": "wrangler d1 migrations apply DB --remote",
    "db:push:staging": "CLOUDFLARE_DATABASE_ID=$CLOUDFLARE_STAGING_DATABASE_ID drizzle-kit push",
    "db:push:prod": "CLOUDFLARE_DATABASE_ID=$CLOUDFLARE_PRODUCTION_DATABASE_ID drizzle-kit push",
    "db:studio:staging": "CLOUDFLARE_DATABASE_ID=$CLOUDFLARE_STAGING_DATABASE_ID drizzle-kit studio",
    "db:studio:prod": "CLOUDFLARE_DATABASE_ID=$CLOUDFLARE_PRODUCTION_DATABASE_ID drizzle-kit studio",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --project unit",
    "test:nuxt": "vitest --project nuxt",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "type": "module",
  "trustedDependencies": [
    "@parcel/watcher",
    "better-sqlite3",
    "esbuild",
    "keytar",
    "node-pty",
    "protobufjs",
    "sharp",
    "tree-sitter-bash",
    "unrs-resolver",
    "workerd"
  ],
  "version": "0.0.0-dev.0"
}
